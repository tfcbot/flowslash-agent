{
  "name": "flowslash-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate:openapi && npm run generate:sdk && npm run export:llms && npx tsx src/index.ts",
    "build": "npm run generate:openapi && npm run generate:sdk && npm run export:llms && npx tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:openapi": "npx tsx scripts/generate-openapi.ts",
    "generate:sdk": "swagger-typescript-api generate -p ./openapi.json -o ./generated/sdk --name ApiClient",
    "export:llms": "npx tsx scripts/export-llms.ts"
  },
  "dependencies": {
    "@composio/core": "^0.1.8-alpha.0",
    "@hono/node-server": "^1.12.0",
    "@hono/swagger-ui": "^0.2.0",
    "@hono/zod-validator": "^0.2.0",
    "@langchain/anthropic": "^0.3.26",
    "@langchain/core": "^0.3.73",
    "@langchain/google-genai": "^0.2.17",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.11",
    "dotenv": "^16.4.0",
    "hono": "^4.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "swagger-typescript-api": "^13.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
